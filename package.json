{
  "name": "@anilkumar779943/new-lfi-ui-aggrid",
  "version": "0.0.13",
  "description": "A personalised ui library",
  "main": "dist/bundle.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "webpack --mode production",
    "start": "set PORT=4200 &&  react-scripts start",
    "storybook": "concurrently 'npm:watch:*'",
    "watch:storybook": "storybook dev -p 6006",
    "watch:tailwind": "npx tailwindcss -i ./src/index.css -o ./src/styles/tailwind.css --watch",
    "build:tailwind": "npx tailwindcss -i ./src/index.css -o ./src/styles/tailwind.css",
    "build-storybook": "npm build:tailwind && storybook build"
  },
  "dependencies": {
    "@anilkumar779943/new-lfi-ui-aggrid": "^0.0.9",
    "ag-grid-community": "^32.0.0",
    "ag-grid-react": "^32.0.0",
    "react-scripts": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.11.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.9.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "ts-loader": "^9.5.1",
    "typescript": "^4.9.5",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
